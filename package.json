{
  "name": "birthday_bot",
  "version": "0.1.0",
  "private": true,
  "description": "Discord bot to manage and notify birthdays on your server, designed to be lightweight and easy to self-host.",
  "author": "Wanderson-Valentim",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wanderson-Valentim/birthday-bot-discord.git"
  },
  "bugs": {
    "url": "https://github.com/Wanderson-Valentim/birthday-bot-discord/issues"
  },
  "homepage": "https://github.com/Wanderson-Valentim/birthday-bot-discord#readme",
  "scripts": {
    "start": "node --env-file=.env index.js",
    "dev": "node --env-file=.env --watch index.js",
    "deploy:dev": "node --env-file=.env deploy-commands.js",
    "deploy:global": "node --env-file=.env deploy-commands.js --global",
    "clear:dev": "node --env-file=.env clear-commands.js",
    "clear:global": "node --env-file=.env clear-commands.js --global"
  },
  "keywords": [
    "discord",
    "bot",
    "discordjs",
    "birthday",
    "sqlite",
    "sequelize",
    "node-cron",
    "self-host"
  ],
  "dependencies": {
    "discord.js": "^14.23.2",
    "node-cron": "^4.2.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "eslint": "^9.38.0"
  }
}
